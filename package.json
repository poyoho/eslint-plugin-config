{
  "name": "@poyoho/eslint-plugin-config",
  "version": "0.0.1",
  "description": "eslint plugin for myself",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "files": [
    "dist",
    "docs"
  ],
  "author": "poyoho",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint",
    "test": "jest --testPathIgnorePatterns _work.test",
    "test-compile": "cross-env USE_COMPLIED=1 yarn test",
    "build": "tsc -p tsconfig.json",
    "play": "cd playground/rules && ts-node-dev ./vue.ts",
    "play:log": "cd playground/rules && ts-node-dev ./vue.ts --logserve",
    "playjs": "cd playground/rules && ts-node-dev ./js.ts",
    "playjs:log": "cd playground/rules && ts-node-dev ./js.ts --logserve",
    "playstat": "cd playground/rules && ts-node-dev ./stat.ts",
    "gen": "node ./scripts/gen.js",
    "doc:dev": "vitepress dev docs",
    "doc:build": "vitepress build docs",
    "doc:serve": "vitepress serve docs",
    "release": "node ./scripts/release.js",
    "predeploy": "yarn doc:build",
    "deploy": "gh-pages -d docs/.vitepress/dist"
  },
  "lint-staged": {
    "*.{ts,vue,tsx,jsx}": "eslint --fix"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^4.26.0",
    "vue-eslint-parser": "^7.6.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "eslint": "^7.12.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.8.0-125"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/experimental-utils": "^4.9.1",
    "@typescript-eslint/parser": "^4.26.0",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^5.1.0",
    "gh-pages": "^3.1.0",
    "jest": "^27.0.3",
    "minimist": "^1.2.5",
    "semver": "^7.3.5",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2",
    "vitepress": "^0.13.2",
    "vue-eslint-parser": "^7.6.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC"
}
